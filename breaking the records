#include <bits/stdc++.h>

using namespace std;

int main(){
int n;
cin >> n;
vector<int> score(n);
for(int i=0;i<n;i++)
{
    cin>>score[i];
    
}
int max1=score[0];
int min2=score[0];
int m=0;
int x=0;
for(int i=1;i<n;i++)
{
    if(score[i]>max1)
    {
       m++;
       max1=score[i];
        
    }
    else 
    if(score[i]<min2)
    {
        x++;
        min2=score[i];
    }
    
}
cout<<m<<" "<<x<<endl;
return 0;
}
