#include <bits/stdc++.h>

using namespace std;

string ltrim(const string &);
string rtrim(const string &);
vector<string> split(const string &);

/*
 * Complete the 'birthday' function below.
 *
 * The function is expected to return an INTEGER.
 * The function accepts following parameters:
 *  1. INTEGER_ARRAY s
 *  2. INTEGER d
 *  3. INTEGER m
 */

int main()
{
    int t;
    cin>>t;
    int a[t];
    for(int i=0;i<t;i++)
    {
        cin>>a[i];
    }
    int d,m;
    cin>>d>>m;
    int flag=0;
    
    for(int i=0;i<t;i++)
    {
        for(int j=i+1;j<t;j++)
        {
            if((a[j]!=0)&&(a[i]!=d))
            {
                if(a[i]+a[j]==d)
                {
                    flag++;
                    
                }
            }
            else{
                flag=1;
            } 
                
               
               
        }
        
      break; 
    }
   if(flag!=m)
   {
       cout<<flag<<endl;
   }
  
   
   else {
   cout<<m<<endl;
   }
    
   return 0; 
    
    
}
