#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0)
	{
	    int a,b,c;
	    cin>>a>>b>>c;
	    if((a>b)&&(a>c))
	    {
	        if(b>c)
	        {
	            cout<<b<<endl;
	        }
	        else
	        {
	            cout<<c<<endl;
	        }
	    }
	    else if(b>c&&b>a)
	    {
	        if(c>a)
	        {
	            cout<<c<<endl;
	        }
	        else
	        {
	            cout<<a<<endl;
	        }
	    }
	    else if(c>a &&c>b)
	    {
	        if(a>b)
	        {
	            cout<<a<<endl;
	        }
	        else
	        {
	            cout<<b<<endl;
	        }
	    }
	    t--;
	}
	return 0;
}
