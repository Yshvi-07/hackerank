#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0)
	{
	    int n,k;
	    cin>>n>>k;
	    int flag=0;
	    int a[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }
	    
	    
	    for(int i=0;i<n;i++)
	    {
	       a[i]= a[i]+k;
	       if(a[i]%7==0)
	       {
	           flag++;
	       }
	    }
	    cout<<flag<<endl;
	   t--; 
	}
	return 0;
}
