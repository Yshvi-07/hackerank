#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0)
	{
	    int n;
	    int z=0;
	    cin>>n;
	    for(int i=1;i<=n;i++)
	    {
	        for(int j=1;j<=n;j++)
	        {
	            z=i+j;
	            int s=2*n;
	            if(i+j<=s)
	            {
	                cout<<"1"<<" ";
	            }
	        }
	        cout<<endl;
	    }
	    t--;
	}
	return 0;
}
