#include <bits/stdc++.h>

using namespace std;

int main()

{
    int s,t,a,b,m,n;
    cin>>s>>t>>a>>b>>m>>n;
    int a1[m];
    int a2[n];
    int apple=0;
    int oranges=0;
    for(int i=0;i<m;i++)
    {
        cin>>a1[i];
        a1[i]=a1[i]+a;
        if((a1[i]>=s) && (a1[i]<=t))
        {
            apple=apple+1;
        }
        
    }
    for(int i=0;i<n;i++)
    {
        cin>>a2[i];
        a2[i]=a2[i]+b;
        if((a2[i]>=s) && (a2[i]<=t))
        {
            oranges=oranges+1;
        }
        
    }
    cout<<apple<<endl<<oranges;
    
    return 0;

}
