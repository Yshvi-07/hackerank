#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    int h[26];
    for(int i=0;i<26;i++)
    {
        cin>>h[i];
        
    }
    string str;
    cin>>str;
    int l=str.length();
    int max=0;
    for(int i=0;i<l;i++)
    {
        int n=str[i]-97;
        if(h[n]>max)
        {
            max=h[n];
        }
    }
    int a= l*max;
    cout<<a<<endl;
}
