#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    int d=0;
    int s=0;
    int r=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]>=38)
        {
           d = a[i]/10;
           
           r = (d*10)+5;
           
           if(a[i]>r)
           {
               r=r+5;
           }
           s=r-a[i];
           if(s>3)
           {
               cout<<a[i]<<endl;
           }
           
           
           else if(s<3)
           {
               cout<<r<<endl;
           }
           else if(s==3){
           cout<<a[i]<<endl;
           }
        }
        
        else 
        if(a[i]<38)
        {
            cout<<a[i]<<endl;
        }
    }
        return 0;
        
    }
    
    
    
    
    
    
    
    
    
    
