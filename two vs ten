#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
//	int flag=0;
	while(t>0)
	{
	    int n;
	    cin>>n;
	    if(n%10==0)
	    {
	       cout<<"0"<<endl;
	    }
	    else if(n%5==0) {
	        cout<<"1"<<endl;
	        
	        
	    }
	    else {
	       cout<<"-1"<<endl;
	    }
	        
	    
	    
	    t--;
	   
	}
	return 0;
}
