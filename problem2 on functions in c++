#include <iostream>
#include <cstdio>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int greatest_No(int a,int b,int c,int d)
 {
     
    if(a>b&&a>c&&a>d)
    {
        return a;
    }
    else 
    
    if(b>c&&b>d)
    {
        return b;
    }
    else 
    if(c>d)
    {
        return c;
    }
    
    else
    return d;
}




int main()
{
    int a,b,c,d;
    
    scanf("%d\n",&a);
    scanf("%d\n",&b);
    scanf("%d\n",&c);
    scanf("%d\n",&d);
    
    int ans = greatest_No(a,b,c,d);
    printf("%d",ans);
    return 0;
}
    
    
    
