#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0)
	{
	    int d1,d2,d3,s1,s2,s3;
	    cin>>d1>>d2>>d3>>s1>>s2>>s3;
	    int sum1=0;
	    int sum2=0;
	    sum1=d1+d2+d3;
	    sum2=s1+s2+s3;
	    if(sum1>sum2)
	    {
	        cout<<"Dragon"<<endl;
	    }
	    else if(sum2>sum1)
	    {
	        cout<<"Sloth"<<endl;
	    }
	    else{
	        if(d1>s1)
	        {
	            cout<<"Dragon"<<endl;
	        }
	        else if(s1>d1)
	        {
	            cout<<"Sloth"<<endl;
	        }
	        else{
	            if(d2>s2)
	            {
	                cout<<"Dragon"<<endl;
	            }
	            else if(s2>d2)
	            {
	                cout<<"Sloth"<<endl;
	            }
	            else{
	                cout<<"Tie"<<endl;
	            }
	        }
	    }
	    t--;
	}
	return 0;
}
