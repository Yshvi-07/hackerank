#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0)
	{
	    int a1,a2,b1,b2,c1,c2;
	    cin>>a1>>a2>>b1>>b2>>c1>>c2;
	    int sum1=0;
	    int sum2=0;
	    int sum3=0;
	    sum1=a1+a2;
	    sum2=b1+b2;
	    sum3=c1+c2;
	    if((sum1>sum2)&&(sum1>sum3))
	    {
	        cout<<sum1<<endl;
	    }
	    else if(sum2>sum3){
	        cout<<sum2<<endl;
	    }
	    else
	    {
	        cout<<sum3<<endl;
	    }
	   t--; 
	}
	return 0;
}
