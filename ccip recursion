#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int digitsSum(int n)
{
 if(n==0)
 {
     return 0;
 }
    else return(n%10+digitsSum(n/10));
    
}








int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
    cin>>n;
    cout<<digitsSum(n);
    
    return 0;
}

