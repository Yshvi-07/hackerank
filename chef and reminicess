#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	for(int i=0;i<T;i++)
	{
	    int a,b;
	    cin>>a>>b;
	    if(a<b)
	    {
	        cout<<b<<endl;
	    }
	    else
	    {
	    cout<<a<<endl;
	    }
	    //for maximum number
	    
	       
	  cout<<a+b<<endl;
	    
	    
	    
	}
	return 0;
}
